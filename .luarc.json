{
  "runtime.version": "Lua 5.4",
  "workspace.ignoreSubmodules": false,
  "workspace.library": [
    "typedefs/?.lua"
  ],
  "runtime.path": [
    "src/?.lua",
    "lib/?.lua",
    "analysis/scripts/?.lua"
  ],
  "diagnostics.disable": [
    "trailing-space"
  ],
  "diagnostics.neededFileStatus": {
    "codestyle-check" : "Any",
    "name-style-check": "Opened"
  },
  "diagnostics.ignoredFiles": "Disable",
  "diagnostics.libraryFiles": "Disable"
}